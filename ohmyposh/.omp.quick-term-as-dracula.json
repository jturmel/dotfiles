{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#000000",

    "dracula_background": "#282a36",
    "dracula_foreground": "#f8f8f2",
    "dracula_current-line": "#44475a",
    "dracula_yellow": "#f1fa8c",
    "dracula_comment": "#6272a4",
    "dracula_cyan": "#8be9fd",
    "dracula_green": "#50fa7b",
    "dracula_orange": "#ffb86c",
    "dracula_pink": "#ff79c6",
    "dracula_purple": "#bd93f9",
    "dracula_red": "#ff5555"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:dracula_current-line",
          "foreground": "p:dracula_foreground",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": " {{ .Icon }} ",
          "type": "os"
        },
        {
          "background": "p:dracula_foreground",
          "foreground": "p:dracula_background",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:dracula_comment",
          "foreground": "p:dracula_foreground",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 2,
            "folder_icon": "\u2026",
            "folder_separator_icon": " <transparent>\ue0b1</> "
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:dracula_green",
          "foreground": "p:dracula_background",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:dracula_orange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:dracula_red{{ end }}",
            "{{ if gt .Ahead 0 }}p:dracula_red{{ end }}",
            "{{ if gt .Behind 0 }}p:dracula_red{{ end }}"
          ],
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "branch_icon": "\uf418 ",
            "branch_identical_icon": "\uf444",
            "branch_gone_icon": "\ueab8"
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <#121318>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#121318>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <#121318>\ueb4b {{ .StashCount }}</>{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:dracula_comment",
          "foreground": "p:dracula_foreground",
          "properties": {
            "style": "austin",
            "always_enabled": true
          },
          "invert_powerline": true,
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "p:dracula_current-line",
          "foreground": "p:dracula_foreground",
          "properties": {
            "time_format": "15:04"
          },
          "invert_powerline": true,
          "style": "diamond",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:dracula_current-line",
          "style": "plain",
          "template": "\u2570\u2500 <p:dracula_comment>{{ if .Root }}#{{else}}${{end}}</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
